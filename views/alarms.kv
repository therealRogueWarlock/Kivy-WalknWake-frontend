#:kivy 1.0
#: import COLOUR theme.Colour
#: import ICON theme.Icons
#: import FONTSIZE theme.Text
#: import RESOLUTION theme.Size
<Alarms>:
    BoxLayout:
        orientation: 'vertical'

        Label:
            text:'alarms'



        Button:
            text: 'Create alarm'
            on_press:
                root.manager.transition.direction = 'left'
                root.manager.current = 'set_alarm'

        Button:
            text: 'Go to home'
            on_press:
                root.manager.transition.direction = 'right'
                root.manager.current = 'home'